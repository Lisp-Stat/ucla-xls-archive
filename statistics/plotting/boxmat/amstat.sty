\def\maketitle{\par
 \begingroup
   \def\thefootnote{\fnsymbol{footnote}}
   \def\@makefnmark{\hbox
       to 0pt{$^{\@thefnmark}$\hss}}
   \if@twocolumn
     \twocolumn[\@maketitle]
     \else %\newpage
     \global\@topnum\z@        % Prevents figures from going at top of page.
     \@maketitle \fi
\thispagestyle{empty}
\@thanks
 \endgroup
 \setcounter{footnote}{0}
 \let\maketitle\relax
 \let\@maketitle\relax
\pagestyle{myheadings}\markboth{\@author}{\@runninghead}
		      \markright{\@runninghead}
 \gdef\@thanks{}\gdef\@author{}\gdef\@title{}\let\thanks\relax
}

\def\@maketitle{
{\raggedleft
\if@twocolumn
   \LARGE \bf \@title
\else
   \Large \bf \@title
\fi
\par}
  \vskip 1em		    % Vertical space after title.
\begin{flushleft}
  {\large		%\sc % each author set in \large, in a
   \lineskip .5em	    % tabular environment
   \begin{tabular}[t]{@{}l}
     \@author\thanks\@thanksnote
   \end{tabular} \par}
\end{flushleft}
 \par
}

